  "description": "Updating livershi.com to use newer tech",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel webpack node sass-sqwatch",
    "webpack": "npx webpack -w --mode development",
    "node": "node ./dev/server.js",
    "sass-sqwatch": "npx sass --watch src/css/index.sass docs/index.css",
    "build": "npm-run-all webpack-b sass api",
    "webpack-b": "npx webpack --mode production",
    "sass": "npx sass src/css/Index.sass docs/index.css",
    "api": "npm-run-all japanese sheets",
    "sheets": "python sheets.py",
    "japanese": "node japanese.js"
  },
